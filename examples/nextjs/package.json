{
  "name": "@trpc-routes/example-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "pnpm with-env next dev",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "trpc:generate": "trpc-routes generate",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "start": "pnpm with-env next start",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@t3-oss/env-nextjs": "^0.7.1",
    "@tanstack/react-query": "^4.18.0",
    "@trpc/client": "^10.44.1",
    "@trpc/next": "^10.44.1",
    "@trpc/react-query": "^10.44.1",
    "@trpc/server": "^10.44.1",
    "next": "^14.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "^2.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@trpc-routes/eslint-config": "workspace:^0.1.0",
    "@trpc-routes/generate": "workspace:^0.1.4",
    "@trpc-routes/prettier-config": "workspace:^0.1.0",
    "@trpc-routes/tailwind-config": "workspace:^0.1.0",
    "@trpc-routes/treeshake": "workspace:^0.1.4",
    "@trpc-routes/tsconfig": "workspace:^0.1.0",
    "@types/eslint": "^8.44.8",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "autoprefixer": "^10.4.16",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.55.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@trpc-routes/eslint-config/base",
      "@trpc-routes/eslint-config/nextjs",
      "@trpc-routes/eslint-config/react"
    ]
  },
  "prettier": "@trpc-routes/prettier-config"
}
