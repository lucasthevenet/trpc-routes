{
  "name": "@trpc-routes/treeshake",
  "version": "0.1.4",
  "type": "module",
  "license": "MIT",
  "description": "Compiler plugin for tree-shaking trpc routers",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    },
    "./next": {
      "types": "./dist/next.d.ts",
      "import": "./dist/next.js",
      "default": "./dist/next.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "default": "./dist/vite.cjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.js",
      "default": "./dist/rollup.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.js",
      "default": "./dist/webpack.cjs"
    },
    "./rspack": {
      "types": "./dist/rspack.d.ts",
      "import": "./dist/rspack.js",
      "default": "./dist/rspack.cjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "import": "./dist/esbuild.js",
      "default": "./dist/esbuild.cjs"
    },
    "./runtime": {
      "types": "./dist/runtime.d.ts",
      "import": "./dist/runtime.js",
      "default": "./dist/runtime.cjs"
    }
  },
  "sideEffects": false,
  "typesVersions": {
    "*": {
      "next": [
        "dist/next.d.ts"
      ],
      "vite": [
        "dist/vite.d.ts"
      ],
      "rollup": [
        "dist/rollup.d.ts"
      ],
      "webpack": [
        "dist/webpack.d.ts"
      ],
      "rspack": [
        "dist/rspack.d.ts"
      ],
      "esbuild": [
        "dist/esbuild.d.ts"
      ],
      "runtime": [
        "dist/runtime.d.ts"
      ]
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucasthevenet/trpc-routes.git",
    "directory": "packages/treeshake"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "clean": "rm -rf .turbo dist node_modules",
    "lint": "eslint .",
    "format": "prettier --check \"**/*.{mjs,ts,md,json}\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/core": "^7.23.5",
    "@babel/helper-plugin-utils": "^7.22.5",
    "@babel/plugin-syntax-typescript": "7.23.3",
    "@babel/types": "7.23.5",
    "@rollup/pluginutils": "5.1.0",
    "unplugin": "1.5.1"
  },
  "devDependencies": {
    "@trpc-routes/eslint-config": "0.1.0",
    "@trpc-routes/prettier-config": "0.1.0",
    "@trpc-routes/tsconfig": "0.1.0",
    "@trpc/server": "^10.44.1",
    "@types/babel-types": "^7.0.11",
    "@types/babel__core": "^7.20.0",
    "@types/babel__helper-plugin-utils": "^7.10.0",
    "eslint": "^8.55.0",
    "tsup": "7.2.0",
    "typescript": "^5.3.2"
  },
  "eslintConfig": {
    "extends": [
      "@trpc-routes/eslint-config/base"
    ]
  },
  "prettier": "@trpc-routes/prettier-config"
}
